{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"skipLibCheck": true,
		"types": ["node"],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"outDir": "./dist",
		"baseUrl": "./src",
		"paths": {
			"@/*": ["./*"],
			"@/env": ["./env"]
		}
	},
	"exclude": ["node_modules"]
}
