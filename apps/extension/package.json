{
	"name": "@envval/extension",
	"displayName": "envval",
	"description": "",
	"version": "1.0.2",
	"icon": "./assets/icons/envval.png",
	"engines": {
		"vscode": "^1.106.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "envval.showQuickSyncAction",
				"title": "EnvVal: Show Quick Sync Action"
			},
			{
				"command": "envval.openStatus",
				"title": "EnvVal: Open Status"
			},
			{
				"command": "envval.reauthenticate",
				"title": "EnvVal: Re-authenticate"
			},
			{
				"command": "envval.forceSync",
				"title": "EnvVal: Force Sync"
			},
			{
				"command": "envval.showLogs",
				"title": "EnvVal: Show Logs"
			},
			{
				"command": "envval.logout",
				"title": "EnvVal: Logout"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.19.3",
		"@types/vscode": "^1.107.0",
		"@vscode/test-cli": "^0.0.12",
		"@vscode/test-electron": "^2.5.2",
		"esbuild": "^0.27.2",
		"eslint": "^9.39.2",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0"
	},
	"dependencies": {
		"axios": "^1.13.2"
	}
}
